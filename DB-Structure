CREATE TABLE CUSTOMER_463730 (
CUSTOMERID NUMBER(4) primary key,
FIRSTNAME VARCHAR2(30) NOT NULL,
LASTNAME VARCHAR2(30) NOT NULL,
DOB DATE NOT NULL,
GENDER VARCHAR2(7) NOT NULL,
CITY VARCHAR2(50) NOT NULL,
COUNTRY VARCHAR2(25) NOT NULL,
EMAIL VARCHAR2(50) NOT NULL,
ANNUAL_SALARY NUMBER(20,5) NOT NULL
);

CREATE SEQUENCE custSeq
MINVALUE 100
MAXVALUE 1000
START WITH 100
INCREMENT BY 1;

SELECT custSeq.nextval from dual;
